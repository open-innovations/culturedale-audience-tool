		{{ comp.oi.map.hex_cartogram({ "config": {
			"hexjson": {
				"layout":"odd-r",
				"hexes": {
					"E05001372":{"sn": "CAL", "n":"Calder","q":0,"r":3,"colour":"#0DBC37"},
					"E05001376":{"sn": "IMX", "n":"Illingworth & Mixenden","q":2,"r":4,"colour":"#0DBC37"},
					"E05001385":{"sn": "TOD", "n":"Todmorden","q":0,"r":2,"colour":"#0DBC37"},
